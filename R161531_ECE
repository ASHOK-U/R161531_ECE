#include <stdio.h>

int main()
{
	int hindi;
		printf("please enter the num 1 to 6");
	("consider here 1= monday ,and 6= saturday");
		scanf("%d",&maths);
	if(hindi == 1)
	{
		printf(\n today is monday\n8am-10th\n1pm-6th\n2pm-7th\n3pm-8th\n4pm-9th");
	}
	else if (hindi == 2)
	{
		printf(\ntoday is tuesday\n8am-9thth\n9am-10th\n2pm-6th\n3pm-7th\n4pm-8th");
	}
	else if (hindi == 3)
	{
		printf("\n today is wednesday\n8am-8th\n9am-9th\n10am-10th\n3pm-6th\n4pm-7th");
	}
	else if (hindi == 4)
	{
		printf("\n today is thursday\n8am-7th\n9am-8th\n10am-9th\n11am-10th\n4pm-6th");
	}
	else if (hindi == 5)
	{
		printf("\n today is friday\n8am-6th\9am-7th\10am-8th\n11am-9th\n12pm-10th");
	}
	else if (hindi == 6)
	{
		printf("\n today is saturday\n9am-6th\n10am-7th\n11am-8th\n12pm-9th\n1pm-10th");
	}
