#include <stdio.h>

int main()
{
	int maths;
		printf("please enter the num 1 to 6");
	("consider here 1= monday ,and 6= saturday");
		scanf("%d",&maths);
	if(math==1)
	{
		printf(\n today is monday\n9am-6th\n10am-7th\n11am-8th\n12pm-9th\n1pm-10th");
	}
	else if (maths == 2)
	{
		printf(\ntoday is tuesday\n10am-6th\n11am-7th\n12pm-8th\n1pm-9th\n2pm-10th");
	}
	else if (maths == 3)
	{
		printf("\n today is wednesday\n11am-6th\n12pm-7th\n1pm-8th\n2pm-9th\n3pm-10th");
	}
	else if (maths == 4)
	{
		printf("\n today is thursday\n12pm-6th\n1pm-7th\n2pm-8th\n3pm-9th\n4pm-10th");
	}
	else if (math == 5)
	{
		printf("\n today is friday\n8am-10th\n1pm-6th\n2pm-7th\n3pm-8th\n4pm-9th");
	}
	else if (math == 6)
	{
		printf("\n today is saturday\n8am-9th\n9am-10th\n2pm-8th\n3pm-7th\n4pm-8th");
	}
	else
		printf(\n enter the valued num between 1 to 6);
	return 0;
}
