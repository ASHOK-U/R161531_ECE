#include <stdio.h>

int main()
{
	int english;
		printf("please enter the num 1 to 6");
	("consider here 1= monday ,and 6= saturday");
		scanf("%d",&maths);
	if(english == 1)
	{
		printf(\n today is monday\n11am-6th\n12pm-7th\n1pm-8th\n2pm-9th\n3pm-10th);
	}
	else if (english == 2)
	{
		printf(\ntoday is tuesday\n12pm-6th\n1pm-7th\n2pm-8th\n3pm-9th\n4pm-10th");
	}
	else if (english == 3)
	{
		printf("\n today is wednesday\n8am-10th1pm-6th\n2pm-7th\n3pm-8th\n4pm-9th");
	}
	else if (english == 4)
	{
		printf("\n today is thursday\n8am-9th\n9am-10th\n2pm-6th\n3pm-7th\n4pm-8th");
	}
	else if (english == 5)
	{
		printf("\n today is friday\n8am-8th\9am-9th\10am-10th\n3pm-6th\n4pm-7th");
	}
	else if (english == 6)
	{
		printf("\n today is saturday\n8am-7th\n9am-8th\n10am-9th\n11am-10th\n4pm-6th");
	}
	else
		printf(\n enter the valued num between 1 to 6);
	return 0;
}
